<!-- 这个xml文件是PC端站点路由 -->
<site>
    <!-- module 路由定义
        @attribute controller 控制器
        @attribute id appid
    -->
    <module controller='site_sso' id='sso'>
        <!--
            @value 路由名称定义 http://xxxx.com/hello.html
        -->
        <name>sso</name>
        <!--
            @value 路由名称备注
        -->
        <title>vmcshop单点登录</title>
    </module>
</site>
