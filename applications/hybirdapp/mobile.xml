<!-- 这个xml文件是移动H5端站点路由 -->
<site>
    <!-- module 路由定义
        @attribute controller 控制器
        @attribute id appid
    -->
    <module controller='mobile_bridge'>
        <!--
            @value 路由名称定义 http://xxxx.com/m/hello.html
        -->
        <name>jsbridge</name>
        <!--
            @value 路由名称备注
        -->
        <title>jsbridge</title>
    </module>

</site>
