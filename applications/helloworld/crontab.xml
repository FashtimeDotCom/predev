<!--该xml文件用来定义系统定时执行任务-->
<cronentries>
    <!-- @arrtibute  定时任务处理类 -->
    <cron id="helloworld_crontab_demoone">
        <!-- @value 任务名称 -->
        <description>定时任务one</description>
        <!-- @value 默认运行频率,定时语法同Linux crontab 定时语法 -->
        <schedule>*/30 * * * *</schedule>
        <!-- @value  默认开启 -->
        <enabled>true</enabled>
    </cron>
    <!-- @arrtibute  定时任务处理类 -->
    <cron id="helloworld_crontab_demotwo">
        <!-- @value 任务名称 -->
        <description>定时任务two</description>
        <!-- @value 默认运行频率,定时语法同Linux crontab 定时语法 -->
        <schedule>*/50 * * * *</schedule>
        <!-- @value  默认开启 -->
        <enabled>true</enabled>
    </cron>
</cronentries>
