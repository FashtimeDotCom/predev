    </service>
    <service id="site_view_helper">
        <class orderby='999'>vmcocean_vhelper</class>
    </service>
    <service id="mobile_view_helper">
        <class orderby='999'>vmcocean_vhelper</class>
    </service>
    <service id="member.create_after">
